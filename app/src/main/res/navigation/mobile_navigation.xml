        tools:layout="@layout/fragment_wish_list" />

    <fragment
        android:id="@+id/nav_search"
        android:name="com.example.impressionselling.ui.search.SearchFragment"
        android:label="Search"
        tools:layout="@layout/fragment_search" />

    <fragment
        android:id="@+id/nav_photo"
        android:name="com.example.impressionselling.ui.photo.photoFragment"
        android:label="Photo"
        tools:layout="@layout/fragment_photo" />


    <fragment
        android:id="@+id/nav_subCategory"
        android:name="com.example.impressionselling.ui.subCategory.subCategoryFragment"
        android:label="Sub-Category"
        tools:layout="@layout/fragment_sub_category" />


    <fragment
        android:id="@+id/nav_generated"
        android:name="com.example.impressionselling.rmo.GeneratedFragment"
        android:label="Generated"
        tools:layout="@layout/fragment_generated" />


    <fragment
        android:id="@+id/nav_completed"
        android:name="com.example.impressionselling.rmo.CompletedFragment"
        android:label="Completed"
        tools:layout="@layout/fragment_completed" />


    <fragment
        android:id="@+id/nav_issued"
        android:name="com.example.impressionselling.rmo.IssuedFragment"
        android:label="Issued"
        tools:layout="@layout/fragment_issued" />

</navigation>