<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_home"
    tools:ignore="UnusedNavigation">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.example.impressionselling.ui.home.HomeFragment"
        android:label="@string/menu_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/nav_orders"
        android:name="com.example.impressionselling.ui.orders.OrderFragment"
        android:label="Orders"
        tools:layout="@layout/fragment_order" />

    <fragment
        android:id="@+id/nav_profile"
        android:name="com.example.impressionselling.ui.profile.ProfileFragment"
        android:label="Your Profile"
        tools:layout="@layout/fragment_profile" />

    <fragment
        android:id="@+id/nav_cart"
        android:name="com.example.impressionselling.ui.cart.CartFragment"
        android:label="My Cart"
        tools:layout="@layout/fragment_cart" />


    <fragment
        android:id="@+id/nav_products"
        android:name="com.example.impressionselling.ui.product.ProductsFragment"
        android:label="Products"
        tools:layout="@layout/fragment_products" />


    <fragment
        android:id="@+id/nav_products_details"
        android:name="com.example.impressionselling.ui.productDetails.ProductDetails"
        android:label="Products Detail"
        tools:layout="@layout/fragment_product_details" />


    <fragment
        android:id="@+id/nav_order_detail"
        android:name="com.example.impressionselling.ui.orders.orderList"
        android:label="Order Details"
        tools:layout="@layout/fragment_order_list" />

    <fragment
        android:id="@+id/nav_wishList"
        android:name="com.example.impressionselling.ui.wish.WishList"
